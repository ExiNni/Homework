package com.kh.example.practice6.run;

public class CakeRun {

	public static void main(String[] args) {
		Cake snack = new Cake();
        snack.setKind("빵");
        snack.setName("케이크");
        snack.setFlavor("블루베리");
        snack.setNumOf(1);
        snack.setPrice(15000);

        System.out.println(snack.information());
    }
}